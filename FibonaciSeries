package assessment;

public class FibonaciSeries {

	public static void main(String[] args) {
		int num = 4;
		int f = 1;
		int s = 1;
		System.out.println(f);
		System.out.println(s);
		for (int i = 3; i <=num; i++) {
			int t = f+s;
			System.out.println(t);
			f=s;
			s=t;
			
		}
		
			
		}

	}

