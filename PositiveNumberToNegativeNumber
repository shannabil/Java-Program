package assessment;

public class PositiveNumberToNegativeNumber {

	public static void main(String[] args) {
		int num =-25;
		if (num>0) {
			System.out.println("The Given Number Positive");
		}
		else {
			int sum = -1*(num);
			System.out.println("The positive for given number is"+sum);
		}

	}

}
