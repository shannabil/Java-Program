package assessment;

public class FindIntersection {

	public static void main(String[] args) {
		int[] ar1= {1,3,4,5,8};
		int[] ar2= {1,3,6,8,5};
		for (int i = 0; i < ar2.length; i++) {
			for (int j = 0; j < ar2.length; j++) {
				if (ar1[i]==ar2[j]) {
					System.out.println(ar1[i]);
					
				}
			}
		}
	}

}
