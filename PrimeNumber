package assessment;

public class PrimeNumber {

	public static void main(String[] args) {
		int num = 2;
		boolean prime = true;
		for (int i = num; i >=1; i--) {
			if (num%2==0) {
				prime = false;
				
		}}if (prime) {
			System.out.println("Given Number Is Prime number");
			
		}
		else {
			System.out.println("Given Number Is Not Prime Number");
		}
			
		}

	}


