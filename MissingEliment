package assessment;

public class MissingEliment {

	public static void main(String[] args) {
		int[] ar1 = {1,2,3,5,7,8,10,11,13,14,16,18,20};
		for (int i = 0; i < ar1.length-1; i++) {
			if((ar1[i]+1) != (ar1[i+1])) {
				System.out.println(ar1[i]+1);
			}
		}
		
		/*
		 * for (int i = 0; i < ar1.length; i++) { int sum = ar1[i]+1;
		 * 
		 * if (sum!= ar1[i]) { System.out.println(sum); } }
		 */
	}

}
